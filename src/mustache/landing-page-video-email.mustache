    height: 60%;
    width: 100%;
    bottom: 0;
    left: 0;
    z-index: -1;
  }

  /* Cards */
  .card {
    position: relative;
  }

  .card::after {
    position: absolute;
    content: '';
    top: -1px;
    height: calc(100% + 1px);
    width: 50vw;
    background-color: white;
  }

  @media (max-width: 1023.98px) {
    .card::after {
      right: calc(100% - 3px );
    }
  }

  @media (min-width: 1024px) {
    .card::after {
      left: calc(100% - 3px );
    }
  }

  /* Accordion */
  .accordion-item {
    border-bottom: 1px solid #E8E8F5 !important;
    padding: 0.5rem 0 !important;
  }

  /* Illustration piece */
  .illustration {
    position: relative;
    z-index: 1;
  }

  @media (max-width: 1023.98px) {
    .illustration {
      width: 6.5rem;
      transform: scaleX(-100%);
    }
  }

  /* IA Forms */
  .opa-interview {
    background-color: transparent !important;
  }

  .opa-submit {
    margin: 0 auto !important;
  }

  @media (min-width: 575px) {
    .opa-interview-controls > div:first-child > .opa-container-horizontal:first-child > div {
      flex: 1 1 0 !important;
    }
  }
