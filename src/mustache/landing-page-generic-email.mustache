    background-size: contain;
    height: 1rem;
    width: 1rem;
    margin-right: 1rem;
    position: relative;
    top: 0.2rem;
  }

  .nav-link::after {
    content: inherit !important; 
  }

  @media (max-width: 1023.98px) {
    .nav-item:first-child {
      display: none !important;
    }
  }

  @media (min-width: 1024px) {
    .nav-item:first-child {
      display: block !important;
    }
  }

/* Hero */
#hero {
  position: relative;
}

#hero::before {
  content: '';
  position: absolute;
  background-image: url("data:image/svg+xml,%3Csvg width='1438' height='55' viewBox='0 0 1438 55' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M-2 55C-2 55 294.838 -41.2685 721.5 22.7917C1098.5 79.3954 1438 13.2248 1438 13.2248V0H-2V55Z' fill='%23FCF0ED'/%3E%3C/svg%3E%0A");
  left: 0;
  top: calc(100% - 2px);
  width: 100%;
  height: 3.74vw;
  background-size: contain;
  background-repeat: no-repeat;
}

/* Illustration piece */
.illustration {
  position: relative;
  z-index: 1;
}

@media (max-width: 1023.98px) {
  .illustration {
    width: 6.5rem;
    transform: scaleX(-100%);
  }
}

/* Cards */
.card.shadow-none {
  position: relative;
}

.card.shadow-none::after {
  position: absolute;
  content: '';
  top: -1px;
  height: calc(100% + 1px);
  width: 50vw;
  background-color: white;
}

@media (max-width: 1023.98px) {
  .card.shadow-none::after {
    right: calc(100% - 3px );
  }
}

@media (min-width: 1024px) {
  .card.shadow-none::after {
    left: calc(100% - 3px );
  }
}

  /* IA Forms */
  .opa-interview {
    background-color: transparent !important;
  }

  .opa-interview > div > div {
    padding: 0 !important;
  }

  .opa-submit {
    margin: 0 auto !important;
  }

  @media (min-width: 575px) {
    .opa-interview-controls > div:first-child > .opa-container-horizontal:first-child > div {
      flex: 1 1 0 !important;
    }
  }

/* Steps */
.how-it-works-steps {
  counter-reset: numbering 0;
}

.how-it-works-steps h3 {
  counter-increment: numbering 1;
  font-size: 1.5rem;
}

.how-it-works-steps h3::before {
  content: counter(numbering) ". ";
}

.how-it-works-steps .container [id]:not(:last-child) .row {
  background-repeat: no-repeat;
}

.how-it-works-steps .btn {
  min-width: 14rem;
}

